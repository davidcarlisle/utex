\input regression-test.tex



\START


\TESTEXP{meaning}{\meaning\Ucharcat}

\TESTEXP{65=A/01}{\expandafter\meaning\Ucharcat65 04}
\TESTEXP{65=A/02}{\expandafter\meaning\Ucharcat65 04}
\TESTEXP{65=A/04}{\expandafter\meaning\Ucharcat65 04}
\TESTEXP{65=A/11}{\expandafter\meaning\Ucharcat65 11}
\TESTEXP{65=A/12}{\expandafter\meaning\Ucharcat65 12}

\TESTEXP{0=null/12}{\expandafter\meaning\Ucharcat0 12 }

\TESTEXP{"1D538 = bbb A/11}{\expandafter\meaning\Ucharcat"1D538 11}
\TESTEXP{"1D538 = bbb A/12}{\expandafter\meaning\Ucharcat"1D538 12}


\edef\z{\Ucharcat"1D538 12}
\def\zz#1#2#3\relax{[#1][#2]}
\TESTEXP{1D538 is one token}{\expandafter\zz\z123\relax}


\def\test#1#2{\bgroup\catcode#2=#1\relax\xtest{#1}{#2}}

\def\xtest#1#2#3{%
\TESTEXP{#2/#1}{%
\if#3\Ucharcat\numexpr#2\relax\numexpr#1\relax
[#1 character code OK]
\else
[#1 character code bad]
\fi
\expandafter\ifx\Ucharcat\numexpr#2\relax\numexpr#1\relax#3%
[#1 catcode OK]
\else
[#1 catcode bad]
\fi}%
\egroup}


